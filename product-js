<script>
var $input = $("#Quantity");
$input.val(1);
// Aumenta ou diminui o valor sendo 0 o mais baixo possÃ­vel
$(".quantity-icon").click(function(){
    if ($(this).hasClass('plus-icon'))
        $input.val(parseInt($input.val())+1);
    else if ($input.val()>=1)
        $input.val(parseInt($input.val())-1);
});
